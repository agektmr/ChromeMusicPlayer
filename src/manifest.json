{
  "manifest_version": 2,
  "name": "Chrome Music Player",
  "version": "0.0.1",
  "permissions": [
    "experimental",
    {
      "mediaGalleries": [
        "read",
        "allAutoDetected"
      ]
    },
    "syncFileSystem"
  ],
  "app": {
    "background": {
      "scripts": [
        "js/filer.js",
        "js/database.js",
        "js/ChromeMusicPlayer.js",
        "js/background.js",
        "js/id3/src/stringutils.js",
        "js/id3/src/bufferedbinaryajax.js",
        "js/id3/src/filereader.js",
        "js/id3/src/base64.js",
        "js/id3/src/id3.js",
        "js/id3/src/id3v1.js",
        "js/id3/src/id3v2.js",
        "js/id3/src/id3v2frames.js",
        "js/id3/src/id4.js"
      ]
    }
  },
  "minimum_chrome_version": "24"
}
